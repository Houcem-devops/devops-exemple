apiVersion: v1
kind: ReplicationController
metadata:
  labels:
    name: mysp
  name: mysp
spec:
  replicas: 2
  template:
    metadata:
      labels:
        name: mysp
    spec:
      containers:
      - image: houcemserver/sp:14
        name: mysp
        ports:
        - name: mysp
          containerPort: 2222    
          
---
# Node Port Service
apiVersion: v1
kind: Service
metadata:
  labels:
    name: mysp
  name: mysp
spec:
  type: NodePort
  ports:
    - port: 2222
      targetPort: 2222
  selector:
    name: myapp
